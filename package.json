{
  "name": "@flitz/controllers",
  "version": "0.0.5",
  "description": "Framework, that sets up controllers and routes for flitz, using decorators.",
  "private": false,
  "main": "lib/index.js",
  "files": [
    "index.d.ts",
    "lib/**/*",
    "!lib/sandbox/**/*",
    "!**/*.map",
    "!**/__tests__/**/*.+(ts|tsx|js)",
    "!lib/setupTests.*"
  ],
  "scripts": {
    "build": "del ./lib && tsc -p ./tsconfig.json",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "lint:fix": "eslint --fix -c .eslintrc.js --ext .ts src",
    "test": "jest",
    "sandbox": "ts-node-dev --respawn ./src/sandbox/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flitz-js/controllers.git"
  },
  "keywords": [
    "flitz",
    "controller",
    "route",
    "annotation",
    "decorator",
    "meta",
    "typescript"
  ],
  "author": "Marcel Joachim Kloubert <marcel.kloubert@gmx.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flitz-js/controllers/issues"
  },
  "homepage": "https://github.com/flitz-js/controllers#readme",
  "devDependencies": {
    "@flitz/tsconfig": "0.3.0",
    "@types/jest": "^26.0.19",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "^12.12.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/eslint-plugin-tslint": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "del-cli": "^3.0.1",
    "eslint": "^7.16.0",
    "eslint-config-flitz": "0.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-unicorn": "^21.0.0",
    "flitz": "0.18.0",
    "jest": "^26.6.3",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.9.0"
  },
  "dependencies": {
    "fast-glob": "^3.2.4"
  }
}
